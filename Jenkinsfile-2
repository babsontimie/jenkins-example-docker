pipeline {
  agent {
        label 'docker-jnlp-agent'
         }
  stages {
    stage('Back-end') {
      agent {
        any { image 'maven:3.8.1-adoptopenjdk-11' }
      }
      steps {
        sh 'mvn --version'
      }
    }
    stage('Front-end') {
      agent {
        any { image 'node:16-alpine' }
      }
      steps {
        sh 'node --version'
      }
    }
  }
}
